<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.examen_departamento_empleado">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MostrarDepartamento"></activity>
        <activity android:name=".MostrarEmpleado" />
        <activity android:name=".EliminarDepartamento" />
        <activity android:name=".EliminarEmpleado" />
        <activity android:name=".ActualizarDepartamento" />
        <activity android:name=".ActualizarEmpleado" />
        <activity android:name=".MenuDepartamento" />
        <activity android:name=".MenuEmpleado" />
        <activity android:name=".RegistrarDepartamento" />
        <activity android:name=".RegistrarEmpleado" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>